{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },

    "NotificatorFactory": {
        "UseComment": false
    },

    "LinkProvider": {
        "ShortenUrls": false,
        "IgnoreServices": ["Yandex Music"]
    },

    "PostParser": {
        "AmpersandNames": ["Gent & Jawns", "Vintage & Morelli", "Slips & Slurs"],
        "Splitters": ["x", "X", "&", "pres."]
    }
}